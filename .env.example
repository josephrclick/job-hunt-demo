# Environment Variables Example
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database Configuration (optional, for direct connections)
POSTGRES_URL=postgres://user:password@host:port/database
POSTGRES_PRISMA_URL=postgres://user:password@host:port/database?pgbouncer=true
POSTGRES_URL_NON_POOLING=postgres://user:password@host:port/database

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: OpenRouter Configuration
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Security Keys
EXTENSION_API_KEY=generate-a-secure-random-key-here
INTERNAL_API_SECRET=generate-another-secure-random-key-here
CRON_SECRET=generate-a-third-secure-random-key-here

# AI Model Configuration
EMBED_MODEL=text-embedding-3-small
OPENAI_MODEL=gpt-4o-mini
JD_EXTRACT_MODEL=gpt-4o-mini
JD_ANALYSIS_MODEL=gpt-4o-mini

# Feature Configuration
CONFIDENCE_THRESHOLD=50

# Node Environment
NODE_ENV=development